function create(){VanillaToasts.create({title:document.body.valueToUse=names[Math.floor(Math.random()*names.length)]||null,text:document.body.valueToUse=credits[Math.floor(Math.random()*credits.length)]+credit,type:"success",icon:"res/profile.png",timeout:2500});setTimeout(create,3E3)};